{
  "ApiClient": {
    "BaseUrl": "https://ultimatehoopersapi.azurewebsites.net/",
    "TimeoutSeconds": 30
  },
  "Jwt": {
    "Key": "your-secret-key-here-replace-with-your-actual-key",
    "Issuer": "undergroundhoopers-api",
    "Audience": "undergroundhoopers-clients",
    "ExpirationHours": 24
  },
  "ConnectionStrings": {
    "UnderGroundhoopersDB": "Server=your-server;Database=UnderGroundhoopersDB;User Id=username;Password=password;TrustServerCertificate=True;",
    "DefaultConnection": "Server=your-server;Database=UnderGroundhoopersDB;User Id=username;Password=password;TrustServerCertificate=True;"
  },
  "BlobStorage": {
    "ughBlobConnectionString": "DefaultEndpointsProtocol=https;AccountName=uhblobstorageaccount;AccountKey=your-account-key;EndpointSuffix=core.windows.net",
    "PostContainerName": "postimage",
    "ProfileContainerName": "profileimage",
    "PrivateRunContainerName": "privaterunimage",
    "CourtContainerName": "courtimage",
    "ProductContainerName": "productimage",
    "PostThumbnailContainerName": "postthumbnailimage",
    "TempVideoFileContainerName": "tempvideofile"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },
  "AllowedHosts": "*"
}